{
  "name": "ragamints",
  "version": "0.1.6",
  "description": "Download Instagram photos while preserving caption, photographer name, geolocation, tags, and creation time (w/ timezone) as EXIF, IPTC, and XMP fields.",
  "scripts": {
    "doc:man": "pandoc -M title=\"ragamints(1)\" -f markdown_github -t man -s README.md  > doc/ragamints.1",
    "doc": "mkdirp doc && npm run doc:man",
    "check:lint": "node --harmony node_modules/eslint/bin/eslint.js index.js bin/ragamints",
    "check:jscs": "node --harmony node_modules/jscs/bin/jscs index.js bin/ragamints",
    "check:jsinspect": "node --harmony node_modules/jsinspect/bin/jsinspect -C index.js bin/ragamints",
    "check": "npm run check:lint && npm run check:jscs",
    "test:jasmine": "node --harmony node_modules/jasmine/bin/jasmine.js",
    "test:jasmine-es6": "node --harmony node_modules/jasmine-es6/bin/jasmine.js",
    "test": "npm run check && npm run test:jasmine-es6",
    "coverage": "node --harmony node_modules/istanbul/lib/cli.js cover node_modules/jasmine-es6/bin/jasmine.js",
    "coverage:coveralls:push": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "publish:pre": "npm prune && npm test && npm run doc",
    "prepublish": "in-publish && npm run publish:pre || true"
  },
  "keywords": [
    "instagram",
    "download",
    "backup",
    "exif",
    "iptc",
    "xmp",
    "caption"
  ],
  "author": {
    "name": "Sebastien Barre",
    "email": "sebastien.barre@gmail.com",
    "url": "http://barre.io"
  },
  "bugs": {
    "url": "https://github.com/sebastienbarre/ragamints/issues",
    "email": "sebastien.barre@gmail.com"
  },
  "repository": {
    "url": "https://github.com/sebastienbarre/ragamints.git",
    "type": "git"
  },
  "man": [
    "./doc/ragamints.1"
  ],
  "engines": {
    "node": ">=0.11.x"
  },
  "license": "BSD-3-Clause",
  "preferGlobal": true,
  "bin": "./bin/ragamints",
  "dependencies": {
    "babel": "^5.6.14",
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "download": "^4.1.2",
    "es6-promise": "^2.3.0",
    "in-publish": "^1.1.1",
    "instagram-node": "^0.5.7",
    "is-iojs": "^1.1.0",
    "moment-timezone": "^0.4.0",
    "node-fetch": "^1.3.0",
    "sugar": "^1.4.1",
    "suspend": "^0.6.1",
    "tz-lookup": "^6.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.19",
    "coveralls": "^2.11.2",
    "eslint": "^0.24.0",
    "eslint-plugin-smells": "^1.0.1",
    "istanbul": "git://github.com/gotwarlost/istanbul.git#harmony",
    "jasmine": "^2.3.1",
    "jasmine-es6": "0.0.14",
    "jscs": "^1.13.1",
    "jsinspect": "^0.6.0",
    "mkdirp": "^0.5.1",
    "rewire": "^2.3.4",
    "strip-ansi": "^2.0.1"
  }
}
