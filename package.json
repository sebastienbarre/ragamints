{
  "name": "ragamints",
  "version": "0.0.0-semantically-released",
  "description": "Download Instagram photos while preserving caption, photographer name, geolocation, tags, and creation time (w/ timezone) as EXIF, IPTC, and XMP fields.",
  "scripts": {
    "commit": "git-cz",
    "doc:man": "pandoc -V title=\"ragamints\" -V section=1 -f markdown_github -t man -s README.md  > doc/ragamints.1",
    "doc:jsdoc": "jsdoc -r lib -d jsdoc",
    "doc": "mkdirp doc && npm run doc:man",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "eslint lib/ bin/ragamints spec/",
    "test:jasmine": "node node_modules/jasmine/bin/jasmine.js",
    "test:jasmine-runner": "node spec/support/jasmine-runner.js",
    "test": "npm run test:jasmine-runner",
    "coverage": "node node_modules/istanbul/lib/cli.js cover -x '**/spec/**' node_modules/jasmine/bin/jasmine.js",
    "coverage:check": "node node_modules/istanbul/lib/cli.js check-coverage --statements 100 --branches 85 --functions 100 --lines 100",
    "coverage:push:codecov": "cat ./coverage/lcov.info | ./node_modules/codecov.io/bin/codecov.io.js",
    "publish:pre": "npm prune && npm run doc",
    "_compile": "babel -d lib/ src/",
    "_prepublish": "npm run compile",
    "prepublish": "in-publish && npm run publish:pre || true",
    "prepush": "([ \"master\" != `git symbolic-ref --short HEAD` ] && true) || (npm run coverage && npm run coverage:check)",
    "_precommit": "npm run lint && npm run test",
    "precommit": "npm run test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "instagram",
    "download",
    "backup",
    "exif",
    "iptc",
    "xmp",
    "caption"
  ],
  "author": {
    "name": "Sebastien Barre",
    "email": "sebastien.barre@gmail.com",
    "url": "http://barre.io"
  },
  "bugs": {
    "url": "https://github.com/sebastienbarre/ragamints/issues",
    "email": "sebastien.barre@gmail.com"
  },
  "repository": {
    "url": "https://github.com/sebastienbarre/ragamints.git",
    "type": "git"
  },
  "man": [
    "./doc/ragamints.1"
  ],
  "engines": {
    "node": ">=0.11.x"
  },
  "license": "BSD-3-Clause",
  "preferGlobal": true,
  "bin": "./bin/ragamints",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-register": "^6.16.3",
    "chalk": "^1.1.3",
    "download": "^5.0.2",
    "es6-promise": "^4.0.3",
    "flickrapi": "^0.6.0",
    "form-data": "^2.1.0",
    "in-publish": "^2.0.0",
    "inquirer": "^1.2.1",
    "instagram-node": "^0.5.8",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.16.2",
    "lz-string": "^1.4.4",
    "moment-timezone": "^0.5.5",
    "nightmare": "^2.7.0",
    "nightmare-load-filter": "^0.2.0",
    "node-fetch": "^1.6.3",
    "node-localstorage": "^1.3.0",
    "openurl": "^1.1.1",
    "qs": "^6.2.1",
    "sort-object": "^3.0.2",
    "store": "^1.3.20",
    "sugar": "^2.0.1",
    "superagent": "^2.3.0",
    "suspend": "^0.7.0",
    "throat": "^3.0.0",
    "tz-lookup": "^6.0.2",
    "vo": "^4.0.2",
    "yargs": "^4.x"
  },
  "devDependencies": {
    "@sebastienbarre/eslint-config": "^3.0.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.8.6",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.2.0",
    "husky": "^0.11.8",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "mkdirp": "^0.5.1",
    "rewire": "^2.5.2",
    "semantic-release": "^4.3.5",
    "strip-ansi": "^3.0.1",
    "temp": "^0.8.3"
  }
}
