#!/usr/bin/env node
const cli = require('../lib/cli');
const core = require('../lib/core');

cli.main(process.argv.slice(2)).then(() => {
  process.exit(0);
}).catch((err) => {
  if (err) {
    core.logger.log(
      core.logger.error(err.error_message || err.message || err.body));
    // core.logger.log(err.stack);
  }
  process.exit(1);
});
